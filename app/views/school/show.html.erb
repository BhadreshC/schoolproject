<br><br>
<h1 align="center"> WELCOME TO <%= %> DASHBOARD OF SCHOOL</h1>
<div class="row">
	<div class="column" style="background-color:#aaa;">
		<h4 align="center">
			NO OF TEACHERS<br>
			<h2 align="center"><%=  %> </h2><br>
			<h3 align="center"><%= link_to "Teacher List", school_teachers_path(@school) %> </h3>
		</h4>
		
	</div>

	<div class="column" style="background-color:#bbb;">
		<h4 align="center">
			NO OF STUDENTS<br>
			<h2 align="center"><%= @no_of_student.count %> </h2><br>
			<h3 align="center"><%= link_to "Student List", school_students_path(@school)%> </h3>
		</h4>
	</div>

	<div class="column" style="background-color:#ccc;">
		<h4 align="center">
			NO OF CLASS<br>
			<h2 align="center"><%=  @classroomscount.count %> </h2><br>
			<h3 align="center"><%= link_to "Class List", school_classrooms_path(@school)%> </h3>
		</h4>
	</div>
</div>
<br>
<div class="princedetail">
	<h1>PRINCIPLE HISTORY</h1><br>
	<table align="center">
		<thead>
			<tr>
				<th>Username</th>
				<th>Email</th>
				<th>Date</th>
				<th>status</th>
			</tr>
		</thead>
		<tbody>
			<% @schoolprinciple.each do |principle| %>
				<tr>
					<td><%= principle.username %></td>
					<td><%= principle.email %></td>
					<td><%= principle.created_at %></td>
					
				</tr>
			<% end %>
		</tbody>
	</table>
</div>
