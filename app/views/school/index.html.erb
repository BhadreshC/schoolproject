<p id="notice"><%= notice %></p>
<h1>SCHOOL</h1>
<table>
	<thead>
		<tr>
			<th>name</th>
			<th>princile</th>
			<th colspan="3">Actions</th>
		</tr>
	</thead>
	<tbody>
		<% @schools.each do |school| %>
			<tr>
				<td><%= school.name %></td>
				<td><%= school.princile %></td>
				<td><%= link_to 'Show', school %></td>
			</tr>
		<% end %>
	</tbody>
</table>
